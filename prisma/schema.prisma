model Municipalities {
  municipality_id   Int         @id @default(autoincrement())
  municipality_name String      @db.VarChar(50)
  department_id     Int
  is_active         Int         @default(1) @db.TinyInt
  created_at        DateTime    @default(now()) @db.Timestamp(0)
  updated_at        DateTime    @default(now()) @db.Timestamp(0)
  Departments       Departments @relation(fields: [department_id], references: [department_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_department_id")
  Zones             Zones[]

  @@index([department_id], map: "FK_department_id")
  @@index([municipality_name], map: "idx_municipalities_name")
}

model Municipio {
  id              Int          @id @default(autoincrement())
  municipio       String       @db.VarChar(50)
  Departamento_id Int
  Departamento    Departamento @relation(fields: [Departamento_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "Municipio_Departamento")
  Zona            Zona[]

  @@index([Departamento_id], map: "Municipio_Departamento")
}

model Projects {
  project_id               Int                @id @default(autoincrement())
  project_name             String             @unique(map: "project_name") @db.VarChar(500)
  project_uuid             String             @unique(map: "project_uuid") @db.VarChar(255)
  project_sold_out         Int                @default(0) @db.TinyInt
  project_primary_market   Int                @default(1) @db.TinyInt
  project_longitude        String             @db.VarChar(100)
  project_latitude         String             @db.VarChar(100)
  project_last_recolection DateTime           @db.Date
  status_id                Int
  subcategory_id           Int
  applications_id          Int
  zone_id                  Int
  is_active                Int                @default(1) @db.TinyInt
  created_at               DateTime           @default(now()) @db.Timestamp(0)
  updated_at               DateTime           @default(now()) @db.Timestamp(0)
  Applications             Applications       @relation(fields: [applications_id], references: [applications_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_applications_id")
  Statuses                 Statuses           @relation(fields: [status_id], references: [status_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_status_id")
  Subcategories            Subcategories      @relation(fields: [subcategory_id], references: [subcategory_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_subcategory_id")
  Zones                    Zones              @relation(fields: [zone_id], references: [zone_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_zone_id")
  ProjectsVersions         ProjectsVersions[]
  Units                    Units[]
  UnitsVersions            UnitsVersions[]

  @@index([applications_id], map: "FK_applications_id")
  @@index([status_id], map: "FK_status_id")
  @@index([subcategory_id], map: "FK_subcategory_id")
  @@index([zone_id], map: "FK_zone_id")
  @@index([project_name], map: "idx_projects_name")
  @@index([project_uuid], map: "idx_projects_uuid")
}

model ProjectsVersions {
  project_version_uuid   String          @id @db.VarChar(200)
  project_id             Int
  project_name           String          @db.VarChar(500)
  project_uuid           String          @db.VarChar(255)
  project_sold_out       Int             @default(0) @db.TinyInt
  project_primary_market Int             @default(1) @db.TinyInt
  status_id              Int
  subcategory_id         Int
  applications_id        Int
  zone_id                Int
  is_active              Int             @default(1) @db.TinyInt
  last_recolection_date  DateTime        @db.Date
  created_at             DateTime        @default(now()) @db.Timestamp(0)
  updated_at             DateTime        @default(now()) @db.Timestamp(0)
  Applications           Applications    @relation(fields: [applications_id], references: [applications_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_projectsversion_applications_id")
  Projects               Projects        @relation(fields: [project_id], references: [project_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_projectsversion_project_id")
  Statuses               Statuses        @relation(fields: [status_id], references: [status_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_projectsversion_status_id")
  Subcategories          Subcategories   @relation(fields: [subcategory_id], references: [subcategory_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_projectsversion_subcategory_id")
  Zones                  Zones           @relation(fields: [zone_id], references: [zone_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_projectsversion_zone_id")
  UnitsVersions          UnitsVersions[]

  @@index([applications_id], map: "FK_applications_id")
  @@index([project_id], map: "FK_projectsversion_project_id")
  @@index([status_id], map: "FK_status_id")
  @@index([subcategory_id], map: "FK_subcategory_id")
  @@index([zone_id], map: "FK_zone_id")
}

model ProyectStatus {
  id       Int        @id @default(autoincrement())
  name     String     @db.VarChar(255)
  Proyecto Proyecto[]
}

model Proyecto {
  Id                  Int           @id @default(autoincrement())
  Name                String        @db.VarChar(500)
  Uuid                String        @db.VarChar(255)
  Vendido             Int           @db.TinyInt
  MercadoPrimario     Int           @db.TinyInt
  SubCategoriaId      Int
  UsoId               Int
  StatusId            Int
  ZonaId              Int
  LastRecolectionDate DateTime      @db.Date
  Country             String        @default("Guatemala") @db.VarChar(24)
  ProyectStatus       ProyectStatus @relation(fields: [StatusId], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_Proyecto_Status")
  SubCategoria        SubCategoria  @relation(fields: [SubCategoriaId], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_Proyecto_SubCategoria")
  Uso                 Uso           @relation(fields: [UsoId], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_Proyecto_Uso")
  Zona                Zona          @relation(fields: [ZonaId], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_Proyecto_Zona")
  Unidades            Unidades[]

  @@index([StatusId], map: "FK_Proyecto_Status")
  @@index([SubCategoriaId], map: "FK_Proyecto_SubCategoria")
  @@index([UsoId], map: "FK_Proyecto_Uso")
  @@index([ZonaId], map: "FK_Proyecto_Zona")
}

model Response {
  ResponseId  String    @id @db.VarChar(100)
  Status      Int       @db.TinyInt
  Environment String    @db.VarChar(11)
  CreatedAt   DateTime  @db.DateTime(0)
  UpdatedAt   DateTime  @db.DateTime(0)
  Comment     Comment[]
}

model Statuses {
  status_id        Int                @id @default(autoincrement())
  status_name      String?            @db.VarChar(255)
  is_active        Int                @default(1) @db.TinyInt
  created_at       DateTime           @default(now()) @db.Timestamp(0)
  updated_at       DateTime           @default(now()) @db.Timestamp(0)
  Projects         Projects[]
  ProjectsVersions ProjectsVersions[]

  @@index([status_name], map: "idx_statuses_name")
}

model SubCategoria {
  id           Int        @id @default(autoincrement())
  subcategoria String     @db.VarChar(100)
  Categoria_id Int
  Proyecto     Proyecto[]
  Categoria    Categoria  @relation(fields: [Categoria_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "SubCategoria_Categoria")

  @@index([Categoria_id], map: "SubCategoria_Categoria")
}

model Subcategories {
  subcategory_id   Int                @id @default(autoincrement())
  subcategory_name String             @db.VarChar(100)
  category_id      Int
  is_active        Int                @default(1) @db.TinyInt
  created_at       DateTime           @default(now()) @db.Timestamp(0)
  updated_at       DateTime           @default(now()) @db.Timestamp(0)
  Projects         Projects[]
  ProjectsVersions ProjectsVersions[]
  Categories       Categories         @relation(fields: [category_id], references: [category_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_category_id")

  @@index([category_id], map: "FK_category_id")
  @@index([subcategory_name], map: "idx_subcategories_name")
}

model Unidades {
  Id                       Int      @id @default(autoincrement())
  Name                     String   @db.VarChar(255)
  CantidadDormitorios      Int
  CantidadSanitarios       Int
  CantidadParqueos         Int
  ValorPropiedad           Int
  PrecioRentaTotal         Int
  PrecioMantenimientoTotal Int?
  SuperficieTotal          Int
  LastRecolectionDate      DateTime @db.Date
  ProyectoId               Int
  Country                  String   @default("Guatemala") @db.VarChar(24)
  Proyecto                 Proyecto @relation(fields: [ProyectoId], references: [Id], onDelete: NoAction, onUpdate: NoAction, map: "FK_Unidad_Proyecto")

  @@index([ProyectoId], map: "FK_Unidad_Proyecto")
}

model Units {
  unit_id                    Int             @id @default(autoincrement())
  unit_name                  String          @db.VarChar(255)
