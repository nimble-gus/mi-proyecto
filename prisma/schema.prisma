generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Applications {
  applications_id   Int                @id @default(autoincrement())
  applications_name String?            @db.VarChar(20)
  params            Json
  is_active         Int                @default(1) @db.TinyInt
  created_at        DateTime           @default(now()) @db.Timestamp(0)
  updated_at        DateTime           @default(now()) @db.Timestamp(0)
  Projects          Projects[]
  ProjectsVersions  ProjectsVersions[]

  @@index([applications_name], map: "idx_applications_name")
}

model Categoria {
  id           Int            @id @default(autoincrement())
  categoria    String         @db.VarChar(100)
  SubCategoria SubCategoria[]
}

model Categories {
  category_id   Int             @id @default(autoincrement())
  category_name String          @db.VarChar(100)
  params        Json?
  is_active     Int             @default(1) @db.TinyInt
  created_at    DateTime        @default(now()) @db.Timestamp(0)
  updated_at    DateTime        @default(now()) @db.Timestamp(0)
  Subcategories Subcategories[]
}

model Comment {
  CommentId  String   @id @db.VarChar(100)
  Comment    String   @db.VarChar(1000)
  ResponseId String   @db.VarChar(100)
  UserId     String   @db.VarChar(100)
  UserName   String   @db.VarChar(100)
  Status     Int      @db.TinyInt
  CreatedAt  DateTime @db.DateTime(0)
  UpdatedAt  DateTime @db.DateTime(0)
  Response   Response @relation(fields: [ResponseId], references: [ResponseId], onDelete: NoAction, onUpdate: NoAction, map: "Comment_Response")

  @@index([ResponseId], map: "Comment_Response")
}

model Configs {
  config_id  Int      @id @default(autoincrement())
  params     Json
  is_active  Int      @default(1) @db.TinyInt
  created_at DateTime @default(now()) @db.Timestamp(0)
  updated_at DateTime @default(now()) @db.Timestamp(0)
}

model Departamento {
  id           Int         @id @default(autoincrement())
  departamento String      @db.VarChar(100)
  Municipio    Municipio[]
}

model Departments {
  department_id   Int              @id @default(autoincrement())
  department_name String           @db.VarChar(100)
  is_active       Int              @default(1) @db.TinyInt
  created_at      DateTime         @default(now()) @db.Timestamp(0)
  updated_at      DateTime         @default(now()) @db.Timestamp(0)
  Municipalities  Municipalities[]

  @@index([department_name], map: "idx_departments_name")
}

