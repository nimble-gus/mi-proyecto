  Projects                   Projects        @relation(fields: [project_id], references: [project_id], onDelete: NoAction, onUpdate: NoAction, map: "FK_project_id")
  UnitsVersions              UnitsVersions[]

  @@index([project_id], map: "FK_project_id")
}

