  fecha_entrega              DateTime?          @db.Date
  meses_de_comercializacion  Int?
  latitud                    Decimal?           @db.Decimal(11, 8)
  longitud                   Decimal?           @db.Decimal(11, 8)
  uso                        String             @db.VarChar(10)
  precio_parqueo             Decimal            @db.Decimal(10, 2)
  parqueos_asignados         Int
  parqueos_para_venta        Int
  parqueos_visitas           Int
  total_parqueos             Int?
  total_m2                   Decimal            @db.Decimal(10, 2)
  total_unidades             Int
  total_m2_disponibles       Decimal            @db.Decimal(10, 2)
  total_unidades_disponibles Int
  enganche                   String?            @db.VarChar(10)
